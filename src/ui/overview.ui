<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk+" version="3.20"/>
  <template class="PhoshOverview" parent="GtkBox">
    <property name="visible">True</property>
    <property name="orientation">vertical</property>
    <child>
      <object class="GtkOverlay">
        <property name="visible">1</property>
        <property name="hexpand">1</property>
        <property name="vexpand">1</property>

        <child>
          <object class="HdyCarousel" id="carousel_running_activities">
            <property name="visible">True</property>
            <property name="orientation">horizontal</property>
            <property name="spacing">18</property>
            <property name="margin_top">16</property>
            <property name="margin_bottom">420</property>
            <property name="valign">fill</property>
            <property name="expand">False</property>
            <property name="reveal-duration">200</property>
            <property name="allow-long-swipes">True</property>
          </object>
        </child>

        <child type="overlay">
          <object class="GtkEventBox" id="evbox_app_grid2">
            <property name="visible">True</property>
            <property name="margin_top">400</property>
            <property name="can_focus">False</property>
            <style>
              <class name="desktop-container"/>
            </style>
            
            <child>
              <object class="GtkLabel" id="my_label">
                <property name="visible">True</property>
                <property name="label">12:34:56</property>
                <property name="use_markup">true</property>
                <property name="selectable">true</property>
                <style>
                  <class name="desktop-clock"/>
                </style>
              </object>
            </child>

          </object>
        </child>

        <child type="overlay">
          <object class="GtkEventBox" id="evbox_app_grid">
            <property name="visible">True</property>
            <property name="margin_top">0</property>
            <property name="can_focus">False</property>
            <child>
              <object class="PhoshAppGrid" id="app_grid">
                <property name="visible">True</property>
              </object>
            </child>
          </object>
        </child>

      </object>
      <packing>
        <property name="pass-through">1</property>
      </packing>
    </child>

    <style>
      <class name="phosh-overview"/>
     </style>
  </template>
</interface>
